{
  "name": "app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently -k -n \"server,client\" -c \"yellow,white\" npm:\"dev:api\" npm:\"dev:ui\"",
    "dev:api": "clear && json-server ./config/server/data.json",
    "dev:ui": "vite",
    "dev:prod": "echo \"TBD\"",
    "test": "echo \"TBD\"",
    "build": "tsc && vite build",
    "unbuild": "rm -rf ./dist",
    "preview": "vite preview",
    "_alias": "./bin/_alias.sh",
    "_onboard": "./bin/_onboard.sh",
    "_latest": "./bin/_latest.sh",
    "delete": "./bin/delete.sh",
    "assist": "cz",
    "load:cl": "commitlint --edit",
    "load:stage": "lint-staged",
    "load:husky": "husky install",
    "load:lighthouse": "lhci autorun",
    "check-all": "npm-run-all check:format check:quality check:type",
    "check:format": "prettier --check \"./src/**/*.{json,(js|ts)}\"",
    "check:quality": "eslint \"./src/**/*.{js,jsx,ts,tsx}\"",
    "check:type": "tsc --project tsconfig.json --pretty --noEmit",
    "apply:format": "prettier --write \"./src/**/*.{json,(js|ts)}\"",
    "apply:quality": "eslint --fix \"./src/**/*.{js,jsx,ts,tsx}\""
  },
  "dependencies": {
    "@codemirror/commands": "^6.1.0",
    "@codemirror/lang-json": "^6.0.0",
    "@codemirror/state": "^6.1.1",
    "@codemirror/view": "^6.2.1",
    "@dnd-kit/core": "^6.0.5",
    "@dnd-kit/modifiers": "^6.0.0",
    "@dnd-kit/sortable": "^7.0.1",
    "@dnd-kit/utilities": "^3.2.0",
    "@hookform/devtools": "^4.2.2",
    "@hookform/resolvers": "^2.9.7",
    "@mantine/core": "^5.2.4",
    "@mantine/hooks": "^5.2.4",
    "@tabler/icons": "^1.90.0",
    "@tanstack/react-query": "^4.2.3",
    "@tanstack/react-query-devtools": "^4.2.3",
    "@uiw/react-codemirror": "^4.11.6",
    "codemirror": "^6.0.1",
    "http-status-codes": "^2.2.0",
    "ky": "^0.31.1",
    "pretty-bytes": "^6.0.0",
    "pretty-ms": "^8.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^3.1.4",
    "react-hook-form": "^7.34.2",
    "react-hot-toast": "^2.3.0",
    "react-router-dom": "^6.3.0",
    "ts-custom-error": "^3.2.2",
    "zod": "^3.18.0",
    "zustand": "^4.1.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.1.1",
    "@commitlint/config-conventional": "^17.1.0",
    "@iconify/react": "^3.2.2",
    "@lhci/cli": "^0.9.0",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.35.1",
    "@typescript-eslint/parser": "^5.35.1",
    "@vitejs/plugin-react": "^2.0.1",
    "commitizen": "^4.2.5",
    "concurrently": "^7.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.23.0",
    "eslint-config-alloy": "^4.6.2",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-editorconfig": "^4.0.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.1",
    "json-server": "^0.17.0",
    "lint-staged": "^13.0.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "typescript": "^4.8.2",
    "vite": "^3.0.9",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-html-config": "^1.0.9",
    "vite-tsconfig-paths": "^3.5.0"
  }
}
